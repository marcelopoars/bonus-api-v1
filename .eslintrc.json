{
  "env": {
    "es2021": true,
    "node": true,
    "commonjs": false
  },
  "extends": [
    "prettier",
    "airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:security/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["import", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true,
        "js": "never"
      }
    ]
  }
}
